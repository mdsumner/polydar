plotem <- function(x, ...) {
l <- polydar(x, ...)
bad <- function(i) {
i <- [i > nrow(x) | i < 1]
}
plot(x, pch = ".")
for (i in seq_along(l)) {
lines(x[bad(l[[i]]), ])
}
}
